  </appSettings>
  <connectionStrings>
    <add name="Galileo" connectionString="Data Source=galileo;Persist Security Info=True;User ID=ASPNETMVC;Password=UV76vPz77v6b258aqtkw;Max Pool Size=400" providerName="System.Data.OracleClient"/>
  </connectionStrings>
  <system.web>
    <customErrors mode="Off"/>
    <httpRuntime maxRequestLength="2097151" useFullyQualifiedRedirectUrl="true" executionTimeout="120"/>
    <!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
